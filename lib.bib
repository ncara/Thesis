  author = {de la de la Mora, Eugenio and Coquelle, Nicolas and Bury, Charles S. and Rosenthal, Martin and Holton, James M. and Carmichael, Ian and Garman, Elspeth F. and Burghammer, Manfred and Colletier, Jacques-Philippe and Weik, Martin},
  options = {useprefix=true},
  date = {2020-02-25},
  journaltitle = {PNAS},
  volume = {117},
