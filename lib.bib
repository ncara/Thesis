  keywords = {Biophysical chemistry,Kinetics,Reaction kinetics and dynamics,SAXS},
  file = {C\:\\Users\\blank\\Zotero\\storage\\X3R9B7XA\\Thompson et al. - 2019 - Temperature-jump solution X-ray scattering reveals.pdf;C\:\\Users\\blank\\Zotero\\storage\\INQG7RUE\\s41557-019-0329-3.html}
