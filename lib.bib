@article{russiConformationalVariationProteins2017,
  title = {Conformational Variation of Proteins at Room Temperature Is Not Dominated by Radiation Damage},
  author = {Russi, S. and González, A. and Kenner, L. R. and Keedy, D. A. and Fraser, J. S. and van den Bedem, H.},
  date = {2017-01-01},
  journaltitle = {J Synchrotron Rad},
  volume = {24},
