  langid = {english},
  keywords = {Nanocrystallography,Nanophotonics and plasmonics},
  file = {C:\Users\blank\Zotero\storage\JIDUESCE\Barty et al. - 2012 - Self-terminating diffraction gates femtosecond X-r.pdf}
}

