  author = {Woodhouse, Joyce and Nass Kovacs, Gabriela and Coquelle, Nicolas and Uriarte, Lucas M. and Adam, Virgile and Barends, Thomas R. M. and Byrdin, Martin and de la Mora, Eugenio and Bruce Doak, R. and Feliks, Mikolaj and Field, Martin and Fieschi, Franck and Guillon, Virginia and Jakobs, Stefan and Joti, Yasumasa and Macheboeuf, Pauline and Motomura, Koji and Nass, Karol and Owada, Shigeki and Roome, Christopher M. and Ruckebusch, Cyril and Schirò, Giorgio and Shoeman, Robert L. and Thepaut, Michel and Togashi, Tadashi and Tono, Kensuke and Yabashi, Makina and Cammarata, Marco and Foucar, Lutz and Bourgeois, Dominique and Sliwa, Michel and Colletier, Jacques-Philippe and Schlichting, Ilme and Weik, Martin},
  options = {useprefix=true},
  date = {2020-02-06},
  journaltitle = {Nat. Commun.},
