  author = {Gil, Agnieszka A. and Laptenok, Sergey P. and French, Jarrod B. and Iuliano, James N. and Lukacs, Andras and Hall, Christopher R. and Sazanovich, Igor V. and Greetham, Gregory M. and Bacher, Adelbert and Illarionov, Boris and Fischer, Markus and Tonge, Peter J. and Meech, Stephen R.},
