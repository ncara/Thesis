@article{kennisPrimaryReactionsLOV22003,
  title = {Primary {{Reactions}} of the {{LOV2 Domain}} of {{Phototropin}}, a {{Plant Blue-Light Photoreceptor}}},
  author = {Kennis, John T. M. and Crosson, Sean and Gauden, Magdalena and van Stokkum, Ivo H. M. and Moffat, Keith and van Grondelle, Rienk},
  options = {useprefix=true},
