  file = {C\:\\Users\\blank\\Zotero\\storage\\WICGSDDP\\Nakajima et al. - 2021 - Transmission of light signals from the light-oxyge.pdf;C\:\\Users\\blank\\Zotero\\storage\\36GU3J9Z\\s41598-021-91497-5.html}
}

@article{nakamuraCoherentOscillationsUltrafast2007,
  title = {Coherent Oscillations in Ultrafast Fluorescence of Photoactive Yellow Protein},
  author = {Nakamura, Ryosuke and Hamada, Norio and Ichida, Hideki and Tokunaga, Fumio and Kanematsu, Yasuo},
