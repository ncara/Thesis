  file = {C\:\\Users\\blank\\Zotero\\storage\\799RFNQW\\Kasahara et al. - 2002 - Photochemical Properties of the Flavin Mononucleot.pdf;C\:\\Users\\blank\\Zotero\\storage\\DY8NIMSV\\762.html}
}

