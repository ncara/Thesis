@article{kasaharaPhotochemicalPropertiesFlavin2002,
  title = {Photochemical {{Properties}} of the {{Flavin Mononucleotide-Binding Domains}} of the {{Phototropins}} from {{Arabidopsis}}, {{Rice}}, {{andChlamydomonas}} Reinhardtii},
  author = {Kasahara, Masahiro and Swartz, Trevor E. and Olney, Margaret A. and Onodera, Akihiko and Mochizuki, Nobuyoshi and Fukuzawa, Hideya and Asamizu, Erika and Tabata, Satoshi and Kanegae, Hiromi and Takano, Makoto and Christie, John M. and Nagatani, Akira and Briggs, Winslow R.},
