@article{royantHelixDeformationCoupled2000,
  title = {Helix Deformation Is Coupled to Vectorial Proton Transport in the Photocycle of Bacteriorhodopsin},
  author = {Royant, Antoine and Edman, Karl and Ursby, Thomas and Pebay-Peyroula, Eva and Landau, Ehud M. and Neutze, Richard},
