@online{wolffMappingProteinDynamics2022,
  title = {Mapping {{Protein Dynamics}} at {{High-Resolution}} with {{Temperature-Jump X-ray Crystallography}}},
  author = {Wolff, Alexander M. and Nango, Eriko and Young, Iris D. and Brewster, Aaron S. and Kubo, Minoru and Nomura, Takashi and Sugahara, Michihiro and Owada, Shigeki and Barad, Benjamin A. and Ito, Kazutaka and Bhowmick, Asmit and Carbajo, Sergio and Hino, Tomoya and Holton, James M. and Im, Dohyun and O’Riordan, Lee J. and Tanaka, Tomoyuki and Tanaka, Rie and Sierra, Raymond G. and Yumoto, Fumiaki and Tono, Kensuke and Iwata, So and Sauter, Nicholas K. and Fraser, James S. and Thompson, Michael C.},
