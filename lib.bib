  author = {Kort, Remco and Ravelli, Raimond B. and Schotte, Friedrich and Bourgeois, Dominique and Crielaard, Wim and Hellingwerf, Klaas J. and Wulff, Michael},
  date = {2003-08},
  journaltitle = {phot},
