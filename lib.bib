  author = {Soares, A. S. and Yamada, Y. and Jakoncic, J. and McSweeney, S. and Sweet, R. M. and Skinner, J. and Foadi, J. and Fuchs, M. R. and Schneider, D. K. and Shi, W. and Andi, B. and Andrews, L. C. and Bernstein, H. J.},
  date = {2022-07-01},
  journaltitle = {Acta Cryst F},
  volume = {78},
