@article{kobayashiRealtimeSpectroscopyTransition2001,
  title = {Real-Time Spectroscopy of Transition States in Bacteriorhodopsin during Retinal Isomerization},
  author = {Kobayashi, Takayoshi and Saito, Takashi and Ohtani, Hiroyuki},
  date = {2001-11},
  journaltitle = {Nature},
  volume = {414},
  number = {6863},
