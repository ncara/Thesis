@article{chengOptimalSampleDelivery2020,
  title = {Towards an {{Optimal Sample Delivery Method}} for {{Serial Crystallography}} at {{XFEL}}},
  author = {Cheng, Robert KY},
  date = {2020-03},
  journaltitle = {Crystals},
  volume = {10},
