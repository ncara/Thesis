@article{cuellar-cruzBiocrystalsPlantsShort2020,
  title = {Biocrystals in {{Plants}}: {{A Short Review}} on {{Biomineralization Processes}} and the {{Role}} of {{Phototropins}} into the {{Uptake}} of {{Calcium}}},
