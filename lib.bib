@article{iulianoUnravelingMechanismLOV2020,
  title = {Unraveling the {{Mechanism}} of a {{LOV Domain Optogenetic Sensor}}: {{A Glutamine Lever Induces Unfolding}} of the {{Jα Helix}}},
  shorttitle = {Unraveling the {{Mechanism}} of a {{LOV Domain Optogenetic Sensor}}},
  author = {Iuliano, James N. and Collado, Jinnette Tolentino and Gil, Agnieszka A. and Ravindran, Pavithran T. and Lukacs, Andras and Shin, SeungYoun and Woroniecka, Helena A. and Adamczyk, Katrin and Aramini, James M. and Edupuganti, Uthama R. and Hall, Christopher R. and Greetham, Gregory M. and Sazanovich, Igor V. and Clark, Ian P. and Daryaee, Taraneh and Toettcher, Jared E. and French, Jarrod B. and Gardner, Kevin H. and Simmerling, Carlos L. and Meech, Stephen R. and Tonge, Peter J.},
  date = {2020-10-16},
  journaltitle = {ACS Chem. Biol.},
  volume = {15},
  number = {10},
  pages = {2752--2765},
  publisher = {American Chemical Society},
  issn = {1554-8929},
  doi = {10.1021/acschembio.0c00543},
  url = {https://doi.org/10.1021/acschembio.0c00543},
  urldate = {2020-12-02},
  abstract = {Light-activated protein domains provide a convenient, modular, and genetically encodable sensor for optogenetics and optobiology. Although these domains have now been deployed in numerous systems, the precise mechanism of photoactivation and the accompanying structural dynamics that modulate output domain activity remain to be fully elucidated. In the C-terminal light–oxygen–voltage (LOV) domain of plant phototropins (LOV2), blue light activation leads to formation of an adduct between a conserved Cys residue and the embedded FMN chromophore, rotation of a conserved Gln (Q513), and unfolding of a helix (Jα-helix) which is coupled to the output domain. In the present work, we focus on the allosteric pathways leading to Jα helix unfolding in Avena sativa LOV2 (AsLOV2) using an interdisciplinary approach involving molecular dynamics simulations extending to 7 μs, time-resolved infrared spectroscopy, solution NMR spectroscopy, and in-cell optogenetic experiments. In the dark state, the side chain of N414 is hydrogen bonded to the backbone N–H of Q513. The simulations predict a lever-like motion of Q513 after Cys adduct formation resulting in a loss of the interaction between the side chain of N414 and the backbone C═O of Q513, and formation of a transient hydrogen bond between the Q513 and N414 side chains. The central role of N414 in signal transduction was evaluated by site-directed mutagenesis supporting a direct link between Jα helix unfolding dynamics and the cellular function of the Zdk2-AsLOV2 optogenetic construct. Through this multifaceted approach, we show that Q513 and N414 are critical mediators of protein structural dynamics, linking the ultrafast (sub-ps) excitation of the FMN chromophore to the microsecond conformational changes that result in photoreceptor activation and biological function.},
  file = {C:\Users\blank\Zotero\storage\IJMS7VLD\Iuliano et al. - 2020 - Unraveling the Mechanism of a LOV Domain Optogenet.pdf}
}

@article{iwataKeyDynamicsConserved2010,
  title = {Key {{Dynamics}} of {{Conserved Asparagine}} in a {{Cryptochrome}}/{{Photolyase Family Protein}} by {{Fourier Transform Infrared Spectroscopy}}},
