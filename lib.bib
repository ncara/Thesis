@article{hartEngineeringPhototropinPhotocycle2019,
  title = {Engineering the Phototropin Photocycle Improves Photoreceptor Performance and Plant Biomass Production},
  author = {Hart, Jaynee E. and Sullivan, Stuart and Hermanowicz, Paweł and Petersen, Jan and Diaz-Ramos, L. Aranzazú and Hoey, David J. and Łabuz, Justyna and Christie, John M.},
