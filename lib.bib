@article{lucicSerialFemtosecondCrystallography2022,
  title = {Serial {{Femtosecond Crystallography Reveals}} the {{Role}} of {{Water}} in the {{One-}} or {{Two-Electron Redox Chemistry}} of {{Compound I}} in the {{Catalytic Cycle}} of the {{B-Type Dye-Decolorizing Peroxidase DtpB}}},
  author = {Lučić, Marina and Wilson, Michael T. and Tosha, Takehiko and Sugimoto, Hiroshi and Shilova, Anastasya and Axford, Danny and Owen, Robin L. and Hough, Michael A. and Worrall, Jonathan A. R.},
