@article{iheeVisualizingReactionPathways2005,
  title = {Visualizing Reaction Pathways in Photoactive Yellow Protein from Nanoseconds to Seconds},
  author = {Ihee, Hyotcherl and Rajagopal, Sudarshan and Šrajer, Vukica and Pahl, Reinhard and Anderson, Spencer and Schmidt, Marius and Schotte, Friedrich and Anfinrud, Philip A. and Wulff, Michael and Moffat, Keith},
  date = {2005-05-17},
  journaltitle = {Proceedings of the National Academy of Sciences},
  volume = {102},
