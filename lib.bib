  file = {C\:\\Users\\blank\\Zotero\\storage\\F3AI9AFC\\Murakawa et al. - 2022 - Serial femtosecond X-ray crystallography of an ana.pdf;C\:\\Users\\blank\\Zotero\\storage\\HA94LYVV\\lp5063sup1.pdf}
}

