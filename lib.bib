  author = {Schotte, Friedrich and Cho, Hyun Sun and Kaila, Ville R. I. and Kamikubo, Hironari and Dashdorj, Naranbaatar and Henry, Eric R. and Graber, Timothy J. and Henning, Robert and Wulff, Michael and Hummer, Gerhard and Kataoka, Mikio and Anfinrud, Philip A.},
