}

@article{emsleyFeaturesDevelopmentCoot2010,
  title = {Features and Development of {{Coot}}},
  author = {Emsley, P. and Lohkamp, B. and Scott, W. G. and Cowtan, K.},
  date = {2010-04-01},
  journaltitle = {Acta Cryst D},
  volume = {66},
