@article{eatonSingleCrystalSpectra1968,
  title = {Single‐{{Crystal Spectra}} of {{Ferrimyoglobin Complexes}} in {{Polarized Light}}},
  author = {Eaton, William A. and Hochstrasser, Robin M.},
  date = {1968-08-01},
