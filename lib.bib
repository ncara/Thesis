@article{nakasakoDomainOrganizationPlant2020,
  title = {Domain {{Organization}} in {{Plant Blue-Light Receptor Phototropin2}} of {{Arabidopsis}} Thaliana {{Studied}} by {{Small-Angle X-ray Scattering}}},
  author = {Nakasako, Masayoshi and Oide, Mao and Takayama, Yuki and Oroguchi, Tomotaka and Okajima, Koji},
