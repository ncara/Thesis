@article{marinoperezVisualizingProteinBreathing2022,
  title = {Visualizing Protein Breathing Motions Associated with Aromatic Ring Flipping},
  author = {Mariño Pérez, Laura and Ielasi, Francesco S. and Bessa, Luiza M. and Maurin, Damien and Kragelj, Jaka and Blackledge, Martin and Salvi, Nicola and Bouvignies, Guillaume and Palencia, Andrés and Jensen, Malene Ringkjøbing},
