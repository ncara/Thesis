@article{mozzanicaPrototypeCharacterizationJUNGFRAU2014,
  title = {Prototype Characterization of the {{JUNGFRAU}} Pixel Detector for {{SwissFEL}}},
  author = {Mozzanica, A. and Bergamaschi, A. and Cartier, S. and Dinapoli, R. and Greiffenberg, D. and Johnson, I. and Jungmann, J. and Maliakal, D. and Mezza, D. and Ruder, C. and Schaedler, L. and Schmitt, B. and Shi, X. and Tinti, G.},
  date = {2014-05},
  journaltitle = {J. Inst.},
  volume = {9},
