@article{liuDeterminingCompleteElectron2013,
  title = {Determining Complete Electron Flow in the Cofactor Photoreduction of Oxidized Photolyase},
  author = {Liu, Zheyun and Tan, Chuang and Guo, Xunmin and Li, Jiang and Wang, Lijuan and Sancar, Aziz and Zhong, Dongping},
  date = {2013-08-06},
  journaltitle = {Proceedings of the National Academy of Sciences},
