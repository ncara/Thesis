@article{timmerTrackingElectronTransfer2023,
  title = {Tracking the {{Electron Transfer Cascade}} in {{European Robin Cryptochrome}} 4 {{Mutants}}},
  author = {Timmer, Daniel and Frederiksen, Anders and Lünemann, Daniel C. and Thomas, Anitta R. and Xu, Jingjing and Bartölke, Rabea and Schmidt, Jessica and Kubař, Tomáš and De Sio, Antonietta and Solov’yov, Ilia A. and Mouritsen, Henrik and Lienau, Christoph},
