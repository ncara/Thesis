}

@article{wranikWatchingReleasePhotopharmacological2023,
  title = {Watching the Release of a Photopharmacological Drug from Tubulin Using Time-Resolved Serial Crystallography},
  author = {Wranik, Maximilian and Weinert, Tobias and Slavov, Chavdar and Masini, Tiziana and Furrer, Antonia and Gaillard, Natacha and Gioia, Dario and Ferrarotti, Marco and James, Daniel and Glover, Hannah and Carrillo, Melissa and Kekilli, Demet and Stipp, Robin and Skopintsev, Petr and Brünle, Steffen and Mühlethaler, Tobias and Beale, John and Gashi, Dardan and Nass, Karol and Ozerov, Dmitry and Johnson, Philip J. M. and Cirelli, Claudio and Bacellar, Camila and Braun, Markus and Wang, Meitian and Dworkowski, Florian and Milne, Chris and Cavalli, Andrea and Wachtveitl, Josef and Steinmetz, Michel O. and Standfuss, Jörg},
  date = {2023-02-17},
