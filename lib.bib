@thesis{wagnerStructuralInsightsMycobacterial2011,
  title = {Structural Insights into Mycobacterial Central Carbon Metabolism: The Catalytic Mechanisms and Regulatory Properties of α-Ketoglutarate Decarboxylase ({{KGD}}).},
  author = {Wagner, Tristan},
  date = {2011-09-23},
  institution = {Université Pierre et Marie Curie},
  location = {Paris},
  abstract = {The -ketoglutarate dehydrogenase (KDH) is a multi-enzymatic complex made of three components (E1o, E2o, E3) and situated at the cross road between the Krebs cycle and nitrogen assimilation. Given its key role, this complex needs a tight regulation to coordinate carbon flux as well as energy production through the Krebs cycle. Corynebacterineae have a peculiar situation in which E2o is absent and its catalytic domain is fused to E1o; moreover, they developed a unique type of control by the FHA (Fork-Head associated, phospho-threonine recognizing) protein GarA, a sort of ‘molecular switch’ controlling glutamate synthesis and regulated by phosphorylation. When unphosphorylated, GarA binds KGD (E1o), the catalytic core of the KDH complex, inhibiting its activity. The goal of this thesis is to decipher the catalytic mechanisms of KGD, as well as the molecular basis of the inhibition by GarA. We solved the crystal structure of Mycobacterium smegmatis KGD in complex with its cofactor thiamine diphosphate. We show for the first time that KGD undergoes large conformational rearrangements through the catalytic cycle, the major being a one-turn translocation of an external helix (E). GarA acts as an inhibitor by binding to this helix through its FHA domain, therefore preventing its translocation. However, the KGD-GarA interaction does not involve any phosphorylated threonine but a phosphate-mimicking aspartate on the E helix, an unprecedented behaviour for a FHA domain protein. The additional E2o-like domain also interacts with the same E helix, the result being a synergistic inhibitory effect. The overall inhibition of the enzyme can be relieved by acetyl-CoA, an allosteric activator that binds to a yet distinct site. The acetyl-CoA action, so far unique to the mycobacterial enzyme, is due to subtle changes in a number of residues that are crucial to the catalytic conformational change.},
  langid = {english}
}

@article{walesPotentialEnergyFree2006,
  title = {Potential {{Energy}} and {{Free Energy Landscapes}}},
  author = {Wales, David J. and Bogdan, Tetyana V.},
  date = {2006-10-01},
  journaltitle = {J. Phys. Chem. B},
  volume = {110},
  number = {42},
  pages = {20765--20776},
  publisher = {American Chemical Society},
  issn = {1520-6106},
  doi = {10.1021/jp0680544},
  url = {https://doi.org/10.1021/jp0680544},
  urldate = {2023-02-13},
  abstract = {Familiar concepts for small molecules may require reinterpretation for larger systems. For example, rearrangements between geometrical isomers are usually considered in terms of transitions between the corresponding local minima on the underlying potential energy surface, V. However, transitions between bulk phases such as solid and liquid, or between the denatured and native states of a protein, are normally addressed in terms of free energy minima. To reestablish a connection with the potential energy surface we must think in terms of representative samples of local minima of V, from which a free energy surface is projected by averaging over most of the coordinates. The present contribution outlines how this connection can be developed into a tool for quantitative calculations. In particular, stepping between the local minima of V provides powerful methods for locating the global potential energy minimum, and for calculating global thermodynamic properties. When the transition states that link local minima are also sampled we can exploit statistical rate theory to obtain insight into global dynamics and rare events. Visualizing the potential energy landscape helps to explain how the network of local minima and transition states determines properties such as heat capacity features, which signify transitions between free energy minima. The organization of the landscape also reveals how certain systems can reliably locate particular structures on the experimental time scale from among an exponentially large number of local minima. Such directed searches not only enable proteins to overcome Levinthal's paradox but may also underlie the formation of “magic numbers” in molecular beams, the self-assembly of macromolecular structures, and crystallization.},
  file = {C\:\\Users\\blank\\Zotero\\storage\\H3ST9L2B\\Wales and Bogdan - 2006 - Potential Energy and Free Energy Landscapes.pdf;C\:\\Users\\blank\\Zotero\\storage\\HBSHSB43\\jp0680544.html}
}

