@article{millerUseSitedirectedTriple1990,
  title = {Use of a Site-Directed Triple Mutant to Trap Intermediates: Demonstration That the Flavin {{C}}(4a)-Thiol Adduct and Reduced Flavin Are Kinetically Competent Intermediates in Mercuric Ion Reductase},
  shorttitle = {Use of a Site-Directed Triple Mutant to Trap Intermediates},
