@article{leidermanTransitionTemperatureDependenceGFP2006,
  title = {Transition in the {{Temperature-Dependence}} of {{GFP Fluorescence}}: {{From Proton Wires}} to {{Proton Exit}}},
  shorttitle = {Transition in the {{Temperature-Dependence}} of {{GFP Fluorescence}}},
